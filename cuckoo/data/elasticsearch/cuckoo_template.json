{
    "template": "cuckoo-reports-*",
    "settings": {
        "index": {
            "mapping.coerce": true,
            "number_of_shards": "1",
            "codec": "best_compression",
            "number_of_replicas": "0"
        }
    },
    "mappings": {
        "cuckoo": {
            "date_detection": false,
            "dynamic_templates": [
                {
                    "keyword": {
                        "mapping": {
                            "type": "keyword",
                            "ignore_above": 32766
                        },
                        "match_mapping_type": "string",
                        "match": "*"
                    }
                },
                {
                    "signatures": {
                        "mapping": {
                            "type": "keyword",
                            "ignore_above": 32766
                        },
                        "path_match": "signatures.marks.call.arguments.*",
                        "path_unmatch": "signatures.marks.call.arguments.registers.*",
                        "match": "*",
                        "match_mapping_type": "long"
                    }
                }
            ],
            "properties": {
                "report_time": {
                    "format": "epoch_second",
                    "type": "date"
                },
                "procmemory": {
                    "type": "nested",
                    "include_in_root": true,
                    "properties": {
                        "regions": {
                           "include_in_root": true,
                           "type": "nested"
                        }
                    }
                }
            }
        }
    }
}
